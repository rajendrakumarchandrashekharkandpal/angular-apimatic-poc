              </xs:annotation>
              <xs:restriction base="xs:string">
                <xs:pattern value="(\\b[A-Za-z0-9._%+-]+@(?:[A-Za-z0-9-]+\\.)+[A-Za-z0-9]{2,}\\b{0,64})" />
