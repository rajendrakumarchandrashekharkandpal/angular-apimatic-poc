              </xs:annotation>
              <xs:restriction base="xs:string">
                <xs:pattern value="([0-9]{10})$" />
                <xs:maxLength value="10" />
                <xs:minLength value="10" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="email">
